#include <stdio.h>
#include <stdlib.h>
//Wap to display the sum of 10 numbers that user entered.
int main()
{
  int num, sum = 0;
  int i = 1;
  while(i<=10)
  {
       printf("Enter a number: ");
       scanf("%d", &num);
       sum = sum + num;
       i++;
  }
  printf("The sum of all the numbers is : %d", sum);
}
