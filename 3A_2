#include <stdio.h>
#include <stdlib.h>
//Find all the factors of a number
int main()
{
    int i=1, num;

    printf("Enter any number to find its factor: ");
    scanf("%d", &num);

    printf("All factors of %d are: \n", num);

    while(i<=num)
    {
        //If the number is exactly divisible by i then i is the factor of num
        if(num%i == 0)
        {
            printf("%d, ", i);
        }
        i++;
    }
}
